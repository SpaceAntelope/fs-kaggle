<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>FsKaggle</AssemblyName>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup>
    <Version>0.1.0</Version>
    <Authors>Ares Lazarus Terzopoulos</Authors>
    <Company>AresLazarus</Company>
    <Product>F# Kaggle</Product>
    <Description>Minimalist jupyter-friendly tool for effort-free downloading of complete datasets or individual files hosted on https://www.kaggle.com/, according to the kaggle API.</Description>
    <Copyright>Ares Lazarus Terzopoulos 2020</Copyright>
    <PackageProjectUrl>https://areslazarus.com/</PackageProjectUrl>
    <PackageTags>fsharp, netcore, NET, kaggle, jupyter, machine-learning</PackageTags>
    <NeutralLanguage>english</NeutralLanguage>
    <RepositoryUrl>https://github.com/SpaceAntelope/fs-kaggle</RepositoryUrl>
    <PackageId>FsKaggle</PackageId>
    <AssemblyName>FsKaggle</AssemblyName>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReleaseNotes>CLI tool available at https://www.nuget.org/packages/FsKaggle.CLI/</PackageReleaseNotes>
    <PackageOutputPath>../nupkg</PackageOutputPath>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Types.fs" />
    <Compile Include="Common.fs" />
    <Compile Include="Reporter.fs" />
    <Compile Include="Kaggle.fs" />
    <Compile Include="Interop.fs" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="4.7.0" />
    <PackageReference Include="TaskBuilder.fs" Version="2.1.0" />
  </ItemGroup>
</Project>